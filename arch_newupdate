# mv /usr/bin/bcompare /usr/bin/bcompare.real
# cat /usr/bin/bcompare
#!/bin/sh
rm "${HOME}/.config/bcompare/registry.dat"
/usr/bin/bcompare.real $@
# chmod a+x /usr/bin/bcompare

